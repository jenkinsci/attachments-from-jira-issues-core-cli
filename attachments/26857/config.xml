  <properties>
    <hudson.model.ParametersDefinitionProperty>
      <parameterDefinitions>
        <hudson.model.ChoiceParameterDefinition>
          <name>P4_BRANCH</name>
          <description>Available Automation Script Branches</description>
          <choices class="java.util.Arrays$ArrayList">
            <a class="string-array">
              <string>//SQA/Imaging/ImageLabTouch/Main/...</string>
              <string>//SQA/Imaging/ImageLabTouch/Int/...</string>
              <string>//SQA/Sandbox/acimensel/Imaging/ImageLabTouch/Int/...</string>
